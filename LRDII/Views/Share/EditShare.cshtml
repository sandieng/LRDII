@model LRDII.Models.SharePriceModel


<h2>Edit Harga Saham</h2>

<h4>Koperasi LRDII</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditShare">
            <div class="form-group">
                <label asp-for="NomorHargaSaham" class="control-label"></label>
                <input asp-for="NomorHargaSaham" class="form-control" />
                <span asp-validation-for="NomorHargaSaham" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Edit" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
